{
  "name": "jitterbit",
  "displayName": "Jitterbit",
  "description": "Syntax support for Jitterbit Script language.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages",
    "Themes"
  ],
  "contributes": {
    "languages": [{
      "id": "jitterbit",
      "aliases": ["Jitterbit Script", "jitterbit"],
      "extensions": [".jitterbit",".jb"],
      "icon": {
        "light": "./icons/jitterbit_icon.svg",
        "dark": "./icons/jitterbit_icon.svg"
      },
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "jitterbit",
      "scopeName": "source.jitterbit",
      "path": "./syntaxes/jitterbit.tmLanguage.json"
    }],
    "themes": [{
      "label": "Jitterbit Dark Theme",
      "uiTheme": "vs-dark",
      "path": "./themes/jitterbit-color-theme.json"
    }]
  }
}
