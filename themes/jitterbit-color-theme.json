{
	"name": "Jitterbit Color Theme",
	"type": "dark",
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"comment.jitterbit",
				"comment.block.jitterbit",
				"comment.line.double-slash.jitterbit"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#677b85"
			}
		},
		{
			"name": "Jitterbit System Variables",
			"scope": [
				"support.variable"
			],
			"settings": {
				"foreground": "#4fb848ec"
			}
		},
		{
			"name": "Jitterbit Global Variables",
			"scope": [
				"variable.name.global.jitterbit"
			],
			"settings": {
				"foreground": "#9dd292f7"
			}
		},
		{
			"name": "Jitterbit Local Variables",
			"scope": [
				"variable.name",
				"variable.name.local.jitterbit"
			],
			"settings": {
				"foreground": "#d99bff"
			}
		},
		{
			"name": "Jitterbit Constants",
			"scope": [
				"constant",
				"constant.numeric.int.jitterbit",
				"constant.numeric.float.jitterbit",
				"constant.language.bool.jitterbit"
			],
			"settings": {
				"foreground": "#dda236"
			}
		},
		{
			"name": "Jitterbit Strings",
			"scope": [
				"string",
				"string.quoted.single.jitterbit",
				"string.quoted.double.jitterbit"
			],
			"settings": {
				"foreground": "#42acf2"
			}
		},
		{
			"name": "Escaped Character",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#7156dc"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#e98282"
			}
		},
		{
			"name": "Jitterbit Operators",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#abb2bf"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#abb2bf"
			}
		},
		{
			"name": "Tags",
			"scope": [
				"entity.name.tag",
				"entity.name.tag.jitterbit"
			],
			"settings": {
				"foreground": "#dda236"
			}
		},
		{
			"name": "API Functions",
			"scope": [
				"support.function",
				"support.function.name.jitterbit",
			],
			"settings": {
				"foreground": "#dc5a90"
			}
		},
		{
			"name": "Jitterbit Logical Functions",
			"scope": [
				"support.function.name.control.jitterbit",
			],
			"settings": {
				"foreground": "#ee97bb"
			}
		}
	]
}
